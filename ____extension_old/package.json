{
  "name": "tanaka-playground",
  "version": "0.1.0",
  "description": "UI component playground for Tanaka",
  "private": true,
  "type": "module",
  "scripts": {
    "playground": "NODE_ENV=development APP_MODE=playground rspack serve --mode development --config rspack.config.ts",
    "build:playground": "NODE_ENV=development APP_MODE=playground BUILD_ENV=development rspack build --mode development --config rspack.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@rspack/cli": "^1.4.6",
    "@rspack/core": "^1.4.6",
    "@types/node": "^24.0.13",
    "@vue/compiler-sfc": "^3.5.17",
    "css-loader": "^7.1.2",
    "html-rspack-plugin": "^6.1.2",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.8.3",
    "vue-loader": "^17.4.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "dependencies": {
    "vue": "^3.5.17"
  }
}
